public class ArrayEleSum{
    public static void main(String []args)
    {
        int a[]={7,10,3,7,9,14,16};
        int sum=0;
        for(int i=0; i<a.length; i++)
        {
            if(a[i]%2==0)
            {
                sum=sum + a[i];
            }
        }
        System.out.println("Sum of even no in array is " + sum);
    }
}
